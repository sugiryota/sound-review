<%=render "/shared/second-header"%>
<div class="contents row">
  
  <div class="container">
    <h2>Log in</h2>
    <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>
      <div class="field">
        <label class="form-text">Eメール</label><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <label class="form-text">パスワード</label><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

     

      <div class="actions">
        <%= f.submit "Log in",class:"log" %>
      </div>
    <% end %>
  </div>
  <div class='right-message'>
    <%= flash[:notice] %>
    <%= flash[:alert] %>
  </div> 
  <video autoplay muted playsinline src="/assets/videos/sign.mp4",playsinline autoplay loop ></video> 
</div>
<%=render "/shared/second-footer"%>
